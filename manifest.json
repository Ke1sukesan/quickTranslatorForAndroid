{
  "applications":
  {
    "gecko":
    {
      "strict_min_version": "60.0a1"
    }
  },

  "author": "Sukesan",

  "background":
  {
    "scripts": ["main.js"]
  },

  "default_locale": "en",

  "description": "__MSG_extensionDescription__",

  "icons":
  {
    "48":   "/icons/quickTranslator_icon_48x48px.png",
    "256":  "/icons/quickTranslator_icon_256x256px.png"
  },

  "manifest_version": 2,

  "name": "Quick translator for Android",

  "options_ui":
  {
    "browser_style": true,
    "open_in_tab":   true,
    "page": "/documents/options/option_page.html"
  },

  "browser_action":
  {
    "browser_style": true,
    "default_icon": 
    {
      "48":   "/icons/quickTranslator_icon_48x48px.png",
      "256":  "/icons/quickTranslator_icon_256x256px.png"
    },
    "default_title": "__MSG_tooltipTitle__"
  },

  "permissions": ["storage", "tabs"],

  "version": "1.1.1"
}